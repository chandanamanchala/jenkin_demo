pipeline {
    agent any

    
    stages {
        stage('Checkout') {
            steps {
               
                checkout scm
            }
        }

        stage('Build') {
            steps {
                echo "Building project..."
                sh 'echo build commands here'
            }
        }

        stage('Test') {
            steps {
                echo "Running tests..."
                sh 'echo test commands here'
            }
        }

        stage('Deploy') {
            steps {
                echo "Deploying..."
                sh 'echo deploy commands here'
            }
        }
    }
}
